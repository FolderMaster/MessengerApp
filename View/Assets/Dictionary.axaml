<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:m="clr-namespace:Model.Interfaces;assembly=Model"
					xmlns:vm="clr-namespace:ViewModel.ViewModels.Pages;assembly=ViewModel"
					xmlns:v="clr-namespace:View">
	<Design.PreviewWith>
		<Border Padding="20">
			<StackPanel>
				<ContentControl ContentTemplate="{StaticResource MenuItemTemplate}">
					<ContentControl.Content>
						<vm:UserViewModel/>
					</ContentControl.Content>
				</ContentControl>
				<ContentControl ContentTemplate="{StaticResource PageTemplate}">
					<ContentControl.Content>
						<vm:UserViewModel/>
					</ContentControl.Content>
				</ContentControl>
			</StackPanel>
		</Border>
	</Design.PreviewWith>
	<DataTemplate x:Key="MenuItemTemplate" x:DataType="vm:PageViewModel">
		<ContentControl Content="{Binding Metadata}"/>
	</DataTemplate>
	<DataTemplate x:Key="MetadataPageTemplate" x:DataType="v:PageMetadata">
		<Grid ColumnDefinitions="Auto, *">
			<PathIcon Grid.Column="0" Width="25" Height="25"
					  Data="{Binding Icon}" Padding="5"/>
			<TextBlock Grid.Column="1" VerticalAlignment="Center"
				   Text="{Binding Name}"/>
		</Grid>
	</DataTemplate>
	<DataTemplate x:Key="MessageTemplate" x:DataType="m:IMessage">
		<StackPanel>
			<Label>
				<Label.Content>
					<MultiBinding StringFormat=" {0}:{1} ">
						<Binding Path="Ip"/>
						<Binding Path="Port"/>
					</MultiBinding>
				</Label.Content>
			</Label>
			<Label Content="{Binding Content}"/>
		</StackPanel>
	</DataTemplate>
	<DataTemplate x:Key="ConnectionTemplate" x:DataType="m:IConnection">
		<StackPanel>
			<Label Content="IP:"/>
			<TextBox Text="{Binding Ip}"/>
			<Label Content="Port:"/>
			<TextBox Text="{Binding Port}"/>
		</StackPanel>
	</DataTemplate>
	<DataTemplate x:Key="ReadonlyConnectionTemplate" x:DataType="m:IConnection">
		<StackPanel>
			<Label Content="IP:"/>
			<TextBox Text="{Binding Ip, Mode=OneWay}" IsReadOnly="True"/>
			<Label Content="Port:"/>
			<TextBox Text="{Binding Port, Mode=OneWay}" IsReadOnly="True"/>
		</StackPanel>
	</DataTemplate>
